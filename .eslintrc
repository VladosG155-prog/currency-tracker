{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb-typescript"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
           "project": "./tsconfig.json"
        },
    "plugins": [
        "@typescript-eslint",
        "react",
        "simple-import-sort"
    ],
    
    "rules": {
        "react/react-in-jsx-scope": "off",
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
    }
}
